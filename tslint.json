{
  "defaultSeverity": "warning",
  "extends": [
    "tslint-config-airbnb",
    "tslint-config-prettier",
    "tslint-react",
    "tslint-react-hooks"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "import-name": false,
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import "
      }
    ],
    "no-namespace": true,
    "strict-boolean-expressions": false,
    "variable-name": [
      true,
      "ban-keywords"
    ]
  }
}